{
	"name": "QuickDark",
	"version": "0.0.1",
	"author": "Jure Poker≈ænik",
	"manifest_version": 2,
	"description": "Make sites darker.",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"https://*/*",
		"http://*/*"
	],
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": [
						"https://www.amazon.com/*",
						"https://www.aliexpress.com/*",
						"file:///*",
						"https://www.messenger.com/*",
						"https://www.facebook.com/*",
						"https://www.google.com/*",
						"https://www.google.si/*"
					],
			"css": ["dark.css"]
		}
	]
}